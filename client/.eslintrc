{
  "extends": ["wesbos"],
  "rules": {
    "no-nested-ternary": 0,
    "no-shadow": 0,
    "react/prop-types": 0,
    "jsx-a11y/control-has-associated-label": 0,
    "no-case-declarations": 0,
    "react-hooks/exhaustive-deps": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
